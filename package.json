{
  "name": "gulp-dev-stack",
  "version": "1.0.0",
  "description": "Basic Gulp DevStack for basic front-end development with customized Bootstrap.",
  "main": "index.js",
  "scripts": {
    "build": "node ./node_modules/gulp/bin/gulp.js -f gulpfile-build.js default",
    "develop": "node ./node_modules/gulp/bin/gulp.js -f gulpfile.js default",
    "format": "prettier --write **/*.{js,md,scss}",
    "gulp": "./node_modules/.bin/gulp",
    "remark": "remark -q . -o",
    "html": "node ./node_modules/gulp/bin/gulp.js -f gulpfile.js html",
    "dataset": "node ./node_modules/gulp/bin/gulp.js -f gulpfile.js dataset",
    "rev": "node ./node_modules/gulp/bin/gulp.js -f gulpfile-build.js revision",
    "css:lint": "node ./node_modules/stylelint/bin/stylelint.js **/*.scss",
    "css:fix": "node ./node_modules/stylelint/bin/stylelint.js --fix **/*.scss",
    "css:unused": "node ./node_modules/stylelint-find-rules/src/bin/stylelint-find-rules.js",
    "html:lint": "node ./node_modules/gulp/bin/gulp.js -f gulpfile-build.js validate",
    "js:lint": "node ./node_modules/eslint/bin/eslint.js '**/*.js'",
    "js:fix": "node ./node_modules/eslint/bin/eslint.js --fix '**/*.js'",
    "text:lint": "node ./node_modules/remark-cli/cli.js -q .",
    "text:fix": "node ./node_modules/remark-cli/cli -q . -o --",
    "release": "release-it",
    "version": "node ./node_modules/auto-changelog/src/index.js -p -t keepachangelog.hbs && npm run text:fix && git add CHANGELOG.md"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "lint-staged": "^9.4.3",
    "popper.js": "^1.16.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@visionappscz/stylelint-config-visionapps": "^1.1.0",
    "@visionappscz/stylelint-config-visionapps-order": "^1.1.0",
    "auto-changelog": "^1.16.2",
    "autoprefixer": "^9.7.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-plugin-loop-optimizer": "^1.4.1",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.7",
    "cssnano": "^4.1.10",
    "eslint": "^6.7.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-concat": "^2.6.1",
    "gulp-data": "^1.3.1",
    "gulp-favicons": "^2.3.1",
    "gulp-google-webfonts": "^2.1.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^6.1.0",
    "gulp-inject": "^5.0.4",
    "gulp-jsbeautifier": "^3.0.1",
    "gulp-merge-json": "^1.3.1",
    "gulp-newer": "^1.4.0",
    "gulp-nunjucks": "^5.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-delete-original": "^0.2.3",
    "gulp-rev-replace": "^0.4.4",
    "gulp-rev-rewrite": "^2.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sri-hash": "^2.2.1",
    "gulp-stylelint": "^10.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-upng": "^1.0.3",
    "husky": "^3.1.0",
    "imagemin-mozjpeg": "^8.0.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.19.1",
    "release-it": "^12.4.3",
    "remark": "^11.0.2",
    "remark-cli": "^7.0.1",
    "remark-lint-match-punctuation": "^0.2.0",
    "remark-preset-lint-consistent": "^2.0.3",
    "remark-preset-lint-recommended": "^3.0.3",
    "stylelint": "^12.0.0",
    "stylelint-find-rules": "^2.2.0",
    "stylelint-high-performance-animation": "^1.3.0",
    "stylelint-order": "^3.1.1",
    "stylelint-scss": "^3.13.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "textlint": "^11.5.0",
    "textlint-rule-common-misspellings": "^1.0.1",
    "textlint-rule-diacritics": "^1.0.0",
    "textlint-rule-no-dead-link": "^4.6.1",
    "textlint-rule-terminology": "^2.0.1",
    "textlint-rule-write-good": "^1.6.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm run js:lint"
    }
  },
  "repository": "https://cebreus@bitbucket.org/cebreus/gulp-dev-stack.git",
  "homepage": "https://bitbucket.org/cebreus/gulp-dev-stack#readme",
  "bugs": {
    "url": "https://bitbucket.org/cebreus/gulp-dev-stack/issues",
    "email": "cebreus@live.com"
  },
  "author": "Jaroslav Vr√°na <cebreus@live.com>",
  "private": false,
  "license": "MIT",
  "keywords": [
    "devstack",
    "gulp",
    "bootstrap"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.md": [
      "remark -qSh . -o",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  }
}
