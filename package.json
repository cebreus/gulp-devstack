{
  "name": "gulp-dev-stack",
  "version": "0.0.0",
  "description": "Basic Gulp dev stack for basic front-end development with customized Bootsrap.",
  "main": "index.js",
  "scripts": {
    "build": "node ./node_modules/gulp/bin/gulp.js -f gulpfile-build.js default",
    "dev": "node ./node_modules/gulp/bin/gulp.js -f gulpfile.js default",
    "csslint": "node ./node_modules/gulp/bin/gulp.js -f gulpfile.js csslint",
    "cssfix": "node ./node_modules/gulp/bin/gulp.js -f gulpfile.js cssfix",
    "format": "prettier --write **/*.{js,md,scss}",
    "gulp": "./node_modules/.bin/gulp",
    "html": "node ./node_modules/gulp/bin/gulp.js -f gulpfile.js html",
    "dataset": "node ./node_modules/gulp/bin/gulp.js -f gulpfile.js dataset",
    "rev": "node ./node_modules/gulp/bin/gulp.js -f gulpfile-build.js revision",
    "stylelint-find-unused-rules": "stylelint-find-rules",
    "css:lint": "stylelint **/*.scss",
    "css:fix": "stylelint --fix **/*.scss",
    "js:lint": "eslint **/*.js",
    "js:fix": "eslint --fix  **/*.js",
    "text:lint": "textlint *.md",
    "text:fix": "textlint --fix *.md"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "lint-staged": "^9.1.0",
    "popper.js": "^1.15.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@visionappscz/stylelint-config-visionapps": "^1.0.0",
    "@visionappscz/stylelint-config-visionapps-order": "^1.0.0",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "browser-sync": "^2.26.7",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-data": "^1.3.1",
    "gulp-favicons": "^2.3.1",
    "gulp-google-webfonts": "^2.1.0",
    "gulp-if": "^2.0.2",
    "gulp-inject": "^5.0.2",
    "gulp-jsbeautifier": "^3.0.0",
    "gulp-merge-json": "^1.3.1",
    "gulp-nunjucks": "^5.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-rev": "^9.0.0",
    "gulp-rev-delete-original": "^0.2.3",
    "gulp-rev-replace": "^0.4.4",
    "gulp-rev-rewrite": "^1.1.4",
    "gulp-sass": "^4.0.2",
    "gulp-sass-glob": "^1.1.0",
    "gulp-sri-hash": "^2.1.0",
    "gulp-stylelint": "^9.0.0",
    "husky": "^2.7.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-scss": "^2.0.0",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0",
    "stylelint-find-rules": "^2.0.0",
    "stylelint-high-performance-animation": "^1.2.1",
    "stylelint-order": "^3.0.0",
    "stylelint-scss": "^3.8.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "textlint": "^11.2.6",
    "textlint-rule-common-misspellings": "^1.0.1",
    "textlint-rule-no-dead-link": "^4.4.2",
    "textlint-rule-terminology": "^1.1.30",
    "textlint-rule-write-good": "^1.6.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged",
      "pre-push": "npm run js:lint && css:lint"
    }
  },
  "repository": "https://cebreus@bitbucket.org/cebreus/gulp-dev-stack.git",
  "homepage": "https://bitbucket.org/cebreus/gulp-dev-stack#readme",
  "bugs": {
    "url": "https://bitbucket.org/cebreus/gulp-dev-stack/issues",
    "email": "cebreus69@gmail.com"
  },
  "author": "Jaroslav Vr√°na <cebreus69@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "keywords": [
    "dev stack",
    "gulp",
    "bootstrap"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix",
      "git add"
    ]
  }
}
